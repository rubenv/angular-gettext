{
  "env": {
    "mocha": true,
    "node": true
  },
  "globals": {
    "angular": "readonly",
    "assert": "readonly",
    "browser": "readonly",
    "by": "readonly",
    "element": "readonly",
    "expect": "readonly",
    "inject": "readonly"
  },
  "rules": {
    "no-bitwise": 2,
    "curly": 2,
    "eqeqeq": 2,
    "keyword-spacing": [
      2,
      {}
    ],
    "no-extend-native": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "ignore",
        "named": "never"
      }
    ],
    "one-var": [
      2,
      "never"
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "quote-props": [
      2,
      "as-needed",
      {
        "keywords": true
      }
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "space-infix-ops": 2,
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ],
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "new-cap": 2,
    "no-caller": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "consistent-this": [
      2,
      "self"
    ],
    "dot-notation": 2,
    "yoda": [
      2,
      "never"
    ]
  }
}
